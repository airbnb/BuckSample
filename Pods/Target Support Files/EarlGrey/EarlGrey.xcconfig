CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/EarlGrey
ENABLE_BITCODE = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) "$(PLATFORM_DIR)/Developer/Library/Frameworks" "${PODS_ROOT}/EarlGrey/EarlGrey"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
OTHER_CFLAGS = -fobjc-arc-exceptions
OTHER_LDFLAGS = $(inherited) -framework "CoreData" -framework "CoreFoundation" -framework "CoreGraphics" -framework "EarlGrey" -framework "Foundation" -framework "IOKit" -framework "QuartzCore" -framework "UIKit" -framework "XCTest"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/EarlGrey
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
